{
  "extends": [
    "airbnb-base",
    "airbnb-typescript/base"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 2016,
    "project": "./tsconfig.json"
  },
  "env": {
    "es6": true
  },
  "plugins": ["@typescript-eslint", "import"],
  "rules": {
    "max-len": ["error", { "code": 120 }]
  }
}
